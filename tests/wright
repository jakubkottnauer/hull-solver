x1 x2 x3 x4 x5

x1 * x1 = x1_2
x2 * x2 = x2_2
x3 * x3 = x3_2
x4 * x4 = x4_2
x5 * x5 = x5_2
x1_2 - x1 = i1
x2_2 + x1 = i2
x3_2 + x1 = i3
x4_2 + x1 = i4
x5_2 + x1 = i5
x2 + x3 = j1
x3 - x2 = j2
x2 - x3 = j3
i1 + j1 = k1
i2 + j2 = k2
i3 + j3 = k3
i4 + j1 = k4
i5 + j1 = k5
x4 + x5 = l1
x5 - x4 = l2
x4 - x5 = l3
k1 + l1 = a
k2 + l1 = a
k3 + l1 = a
k4 + l2 = a
k5 + l3 = a

a in [10,10]
