// http://homepages.math.uic.edu/~jan/Demo/solotarev.html

x y a b

dx_2 - cx = a
dy_2 - cy = a
eax + acb = c
fay + c = a

y * y = y_2
y_2 * y = y_3
x * x = x_2
x_2 * x = x_3
c * y = cy
c * x = cx
a * x = ax
a * y = ay
c * b = cb
d * x_2 = dx_2
d * y_2 = dy_2
x_3 - x_2 = e
y_3 - y_2 = f
f - ay = fay
e - ax = eax
a - cb = acb

a in [-1000,1000]
b in [-1000,1000]
x in [-1000,1000]
y in [-1000,1000]
c in [2,2]
d in [3,3]
x_2 in [-10000,10000]
x_3 in [-10000000,10000000]
y_2 in [-10000,10000]
y_3 in [-10000000,10000000]
cy in [-10000000,10000000]
cx in [-10000000,10000000]
ax in [-10000000,10000000]
ay in [-10000000,10000000]
cb in [-10000000,10000000]
dx_2 in [-10000000,10000000]
dy_2 in [-10000000,10000000]
e in [-10000000,10000000]
f in [-10000000,10000000]
fay in [-10000000,10000000]
eax in [-10000000,10000000]
acb in [-10000000,10000000]
