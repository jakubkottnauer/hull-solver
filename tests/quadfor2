// http://homepages.math.uic.edu/~jan/Demo/quadfor2.html
w1 w2 x1 x2

w1 + w2 = a
w1 * x1 = i1
w2 * x2 = i2
i1 + i2 = c
x1 * x1 = x1_2
x2 * x2 = x2_2
x1_2 * x1 = x1_3
x2_2 * x2 = x2_3
w1 * x1_2 = j1
w2 * x2_2 = j2
j1 + j2 = b
w1 * x1_3 = k1
w2 * x2_3 = k2
k1 + k2 = c

a in [1,1]
b in [0.6666666666,0.6666666666]
c in [0,0]
x1 in [-1,1]
x2 in [-1,1]
w1 in [-1,1]
w2 in [-10,1]
x1_2 in [-100,100]
x1_3 in [-100,100]
x2_2 in [-100,100]
x2_3 in [-100,100]
i1 in [-100,100]
i2 in [-100,100]
j1 in [-100,100]
j2 in [-100,100]
k1 in [-100,100]
k2 in [-100,100]
